<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
         <!-- Sequences for booking number -->
        <record id="dtbs_seq_booking_type" model="ir.sequence.type">
            <field name="name">Reservations</field>
            <field name="code">dtbs.carrent.booking</field>
        </record>
 
        <record id="dtbs_seq_booking" model="ir.sequence">
            <field name="name">Reservation Number</field>
            <field name="code">dtbs.carrent.booking</field>
            <field name="prefix">RES/%(year)s%(month)s/</field>
            <field name="padding">3</field>
            <field name="number_next_actual">1</field>            
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>
    </data>
</openerp>