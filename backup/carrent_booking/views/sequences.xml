<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
         <!-- Sequences for Booking -->
        <record id="dtbs_seq_booking_type" model="ir.sequence.type">
            <field name="name">Booking</field>
            <field name="code">dtbs.carrent.booking</field>
        </record>
 
        <record id="dtbs_seq_booking" model="ir.sequence">
            <field name="name">Booking Number</field>
            <field name="code">dtbs.carrent.booking</field>
            <field name="prefix">BOK/%(year)s%(month)s/</field>
            <field name="padding">3</field>
            <field name="number_next_actual">1</field>            
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Sequences for folio -->
        <record id="dtbs_seq_folio_type" model="ir.sequence.type">
            <field name="name">Car Rental Folio</field>
            <field name="code">dtbs.carrent.folio</field>
        </record>
        <record id="dtbs_seq_folio" model="ir.sequence">
            <field name="name">Car Rental Folio</field>
            <field name="code">dtbs.carrent.folio</field>
            <field name="prefix">Folio/</field>
            <field name="padding">5</field>
        </record>
    </data>
</openerp>


